<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper  namespace="com.design.find">

<insert id="find" parameterType="java.util.HashMap">
insert into find (name)
values (#{name})
</insert>

<select id="showChange" resultType="java.util.HashMap">
select name from find
</select>
</mapper>